//    MSGCLASS=Q,
//    CLASS=A,
//    REGION=2M,
//    NOTIFY=&SYSUID
/*ROUTE  PRINT HOLD
//*
//*   DISASTER.RECOVER.JCLLIB(XCCUTIL2)
//*
//*   SYSLMOD  DD   DISP=SHR,DSN=OEM1.XPED700.LOAD212
//*
//*********************************************************************
//* EXAMPLE JCL TO EXECUTE THE XPEDITER/CICS CUSTOMER UTILITY PROGRAM
//* XCCUTIL WITH THE SYSIN SET TO DD DUMMY TO PRINT
//*********************************************************************
//*
//UTIL    EXEC  PGM=XCCUTIL
//*
//STEPLIB  DD   DISP=SHR,DSN=OEM1.XPED700.LOAD212 /* VERIFY DSN /*
//*
//SYSLIN   DD   DSN=&&SLIN,UNIT=SYSDA,SPACE=(CYL,(2,1)),DISP=(,PASS)
//SYSUT1   DD   UNIT=SYSDA,SPACE=(CYL,(1,1))
//*
//MAINTIN  DD   DISP=SHR,DSN=OEM1.XPED700.LOAD212 /* VERIFY DSN /*
//SYSLMOD  DD   DISP=SHR,DSN=OEM1.XPED700.LOAD212 /* VERIFY DSN /*
//*
//*   REPORT   DD   SYSOUT=*             /* PRINTOUT SET HELD */
//REPORT   DD   SYSOUT=A,DEST=U178       /* PRINTOUT REQUIRED */
//SYSPRINT DD   SYSOUT=*
//SYSUDUMP DD   SYSOUT=*
//*
//SYSIN    DD   DUMMY
//*
//*   OEM1.XPED700.JCLLIB(XCCUTIL2)
//*   DISASTER.RECOVER.JCLLIB(XCCUTIL2)
