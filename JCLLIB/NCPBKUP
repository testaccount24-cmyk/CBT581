//NCPBKUP   JOB (6948),'FRANK WOODS',
//       CLASS=X
/*ROUTE  PRINT RMT49
//*
//*  BACKUP THE NCP LIBRARIES FOR DR USE AT COMDISCO.
//*
//*  ENSURE THAT THE OLD 'VG.WOODS.NCPLOAD' HAS BEEN DELETED
//*  BEFORE RUNNING THIS JOB.
//*  IEBCOPY 'SFT2.AGS1.NCPLOAD' TO 'VG.WOODS.NCPLOAD', THEN
//*  BACKUP VG.WOODS.NCPLOAD TO VG.WOODS.BKUP.NCPLOAD.
//*
//*  ENSURE THAT THE OLD 'VG.WOODS.VTAMLST' HAS BEEN DELETED
//*  BEFORE RUNNING THIS JOB.
//*  IEBCOPY 'SFT1.AGS1.VTAMLST' TO 'VG.WOODS.VTAMLST', THEN
//*  BACKUP VG.WOODS.VTAMLST TO VG.WOODS.BKUP.VTAMLST.
//*
//ALLOC1   EXEC    PGM=IEFBR14
//DD1  DD  DSN=VG.WOODS.NCPLOAD,DISP=(NEW,CATLG,DELETE),
//         DCB=SFT2.AGS1.NCPLOAD,
//         UNIT=SYSDA,SPACE=(TRK,(700,,5))
//*
//COPY1    EXEC    PGM=IEBCOPY,COND=(0,NE)
//SYSPRINT DD      SYSOUT=A
//INFILE1  DD      DSN=SFT2.AGS1.NCPLOAD,DISP=SHR
//OTFILE1  DD      DSN=VG.WOODS.NCPLOAD,DISP=OLD
//SYSUT3   DD      UNIT=SYSDA,SPACE=(TRK,(40,2))
//SYSUT4   DD      UNIT=SYSDA,SPACE=(TRK,(40,2))
//SYSIN    DD      *
  COPY OUTDD=OTFILE1,INDD=((INFILE1,R))
//*
//BACKUP1  EXEC PGM=FDRDSF,COND=(0,NE)
//SYSPRINT  DD  SYSOUT=A
//DISK1     DD  DSN=VG.WOODS.NCPLOAD,DISP=SHR
//TAPE1     DD  DSN=VG.WOODS.BKUP.NCPLOAD,
//             DISP=(NEW,CATLG,DELETE),UNIT=TAPE
//SYSIN  DD  *
 DUMP DSN=VG.WOODS.NCPLOAD
/*
//*
//ALLOC2   EXEC    PGM=IEFBR14
//DD1  DD  DSN=VG.WOODS.VTAMLST,DISP=(NEW,CATLG,DELETE),
//         DCB=SFT1.AGS1.VTAMLST,
//         UNIT=SYSDA,SPACE=(TRK,(100,,30))
//*
//COPY2    EXEC    PGM=IEBCOPY,COND=(0,NE)
//SYSPRINT DD      SYSOUT=A
//INFILE1  DD      DSN=SFT1.AGS1.VTAMLST,DISP=SHR
//OTFILE1  DD      DSN=VG.WOODS.VTAMLST,DISP=OLD
//SYSUT3   DD      UNIT=SYSDA,SPACE=(TRK,(40,2))
//SYSUT4   DD      UNIT=SYSDA,SPACE=(TRK,(40,2))
//SYSIN    DD      *
  COPY OUTDD=OTFILE1,INDD=((INFILE1,R))
//*
//BACKUP2  EXEC PGM=FDRDSF,COND=(0,NE)
//SYSPRINT  DD  SYSOUT=A
//DISK1     DD  DSN=VG.WOODS.VTAMLST,DISP=SHR
//TAPE1     DD  DSN=VG.WOODS.BKUP.VTAMLST,
//             DISP=(NEW,CATLG,DELETE),UNIT=TAPE
//SYSIN  DD  *
 DUMP DSN=VG.WOODS.VTAMLST
