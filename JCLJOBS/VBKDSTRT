//*
//* AGCO D/R TMS BACKUP CA1 TMC
//*
//TMSCOPY  EXEC PGM=TMSCOPY,PARM='BACKUP'
//TMSRPT    DD  SYSOUT=*
//AUDITDD   DD  DSN=OEM1.CA1.AUDIT,DISP=SHR,DCB=BUFNO=3
//DUMPDD    DD  DSN=DISASTER.CA1BKUP.TMC1(+1),
//             DISP=(,CATLG,DELETE),UNIT=TAPE,LABEL=EXPDT=99000,
//             DCB=(MODL.DSCB,DSORG=PS,RECFM=FB,LRECL=340,
//             BLKSIZE=32640,TRTCH=NOCOMP)
//DUMPDD2   DD  DSN=DISASTER.CA1BKUP.TMC2(+1),
//             DISP=(,CATLG,DELETE),UNIT=TAPE,LABEL=EXPDT=99000,
//             DCB=(MODL.DSCB,DSORG=PS,RECFM=FB,LRECL=340,
//             BLKSIZE=32640,TRTCH=NOCOMP)
//AUDUMP    DD  DSN=DISASTER.CA1BKUP.AUDIT(+1),
//             DISP=(,CATLG,DELETE),UNIT=TAPE,LABEL=EXPDT=99000,
//             DCB=(MODL.DSCB,DSORG=PS,RECFM=FB,LRECL=370,BLKSIZE=8880,
//             TRTCH=NOCOMP)
//SYSUDUMP  DD  SYSOUT=*
//*
//* AGCO D/R FDRDSF TMS BACKUP CA1 TMC & AUDIT
//*
//DSFBKP   EXEC PGM=FDRDSF
//SYSPRINT  DD  SYSOUT=*
//DISK1     DD  UNIT=SYSDA,VOL=SER=V90023,DISP=OLD
//TAPE1     DD  DSN=DISASTER.TMSBKUP.TMC(+1),
//             DISP=(,CATLG,DELETE),
//             UNIT=TAPE,
//             EXPDT=99000,
//             DCB=(MODL.DSCB,TRTCH=NOCOMP)
//DISK2     DD  UNIT=SYSDA,VOL=SER=V90005,DISP=OLD
//TAPE2     DD  DSN=DISASTER.TMSBKUP.AUDIT(+1),
//             DISP=(,CATLG,DELETE),
//             UNIT=TAPE,
//             EXPDT=99000,
//             DCB=(MODL.DSCB,TRTCH=NOCOMP)
//SYSIN  DD  *
 DUMP TYPE=DSF
 DUMP DSN=OEM1.CA1.TMC
 DUMP DSN=OEM1.CA1.AUDIT
